{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DataPlane": {
    "DataPlaneId": "local-dev",
    "ControlApiUrl": "http://localhost:5100",
    "PolicySyncInterval": "00:01:00",
